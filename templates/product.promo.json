{
  "sections": {
    "product-form": {
      "type": "template--product",
      "blocks": {
        "media": {
          "type": "media",
          "settings": {
            "image_size": "medium",
            "image-display": "thumbnails",
            "thumbnail_position": "below",
            "enable_image_zoom": true,
            "enable_video_loop": false
          }
        },
        "sale_badge": {
          "type": "sale_badge",
          "settings": {
          }
        },
        "title": {
          "type": "title",
          "settings": {
          }
        },
        "price": {
          "type": "price",
          "settings": {
            "shipping_policy": true,
            "tax_included": false
          }
        },
        "options": {
          "type": "options",
          "settings": {
            "product_variant_style": "swatches",
            "thumbnail_changes_variant": false,
            "hide_soldout_variants": false
          }
        },
        "quantity": {
          "type": "quantity",
          "settings": {
          }
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "settings": {
            "show_smart_checkout": true
          }
        },
        "local_pickup": {
          "type": "local_pickup",
          "settings": {
          }
        },
        "collapsible-tab": {
          "type": "collapsible-tab",
          "settings": {
            "icon": "heart",
            "open_close_icon": "caret",
            "page": ""
          }
        },
        "cross-sells": {
          "type": "cross-sells",
          "settings": {
          }
        },
        "trust_badges": {
          "type": "trust_badges",
          "settings": {
            "trust-badge_max_width": 220,
            "trust-badge": "shopify:\/\/shop_images\/trust-badges.png"
          }
        }
      },
      "block_order": [
        "media",
        "sale_badge",
        "title",
        "price",
        "options",
        "quantity",
        "buy_buttons",
        "local_pickup",
        "collapsible-tab",
        "cross-sells",
        "trust_badges"
      ],
      "settings": {
        "enable_sticky": true,
        "show_out_of_stock_email": true
      }
    },
    "product-description": {
      "type": "template--product--description",
      "settings": {
        "enable_readmore": false
      }
    },
    "rich-text": {
      "type": "text--rich-text",
      "settings": {
        "colors": "accent",
        "button_label": "",
        "button_link": ""
      }
    },
    "accordion": {
      "type": "accordion",
      "blocks": {
        "accordion-0": {
          "type": "tab_content",
          "settings": {
          }
        },
        "accordion-1": {
          "type": "tab_content",
          "settings": {
          }
        },
        "accordion-2": {
          "type": "tab_content",
          "settings": {
          }
        }
      },
      "block_order": [
        "accordion-0",
        "accordion-1",
        "accordion-2"
      ],
      "settings": {
        "colors": "accent",
        "title": "",
        "faq_icon": "caret"
