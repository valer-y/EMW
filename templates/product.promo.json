    "info-columns": {
      "type": "template--product--info-columns",
      "blocks": {
        "info-columns-0": {
          "type": "image_and_text",
          "settings": {
            "heading": "HEADING",
            "button_text": "",
            "button_link": ""
          }
        },
        "info-columns-1": {
          "type": "image_and_text",
          "settings": {
            "heading": "HEADING",
            "button_text": "Learn more",
            "button_link": ""
          }
        },
        "info-columns-2": {
          "type": "image_and_text",
          "settings": {
            "heading": "HEADING",
            "button_text": "Learn more",
            "button_link": ""
          }
        }
      },
      "block_order": [
        "info-columns-0",
        "info-columns-1",
        "info-columns-2"
