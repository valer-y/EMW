      "settings": {
        "colors": "white",
        "heading": "",
        "description": "",
        "columns_per_row": 3,
        "text_alignment": "left",
        "vertical_alignment": "top"
      }
    },
    "image-with-text-overlay": {
      "type": "image--image-with-text-overlay",
      "settings": {
        "image_height": "40vw",
        "bg_image_overlay_opacity": 0,
        "parallax": true,
        "colors": "dark",
        "textPosition": "left",
        "textAlignment": "left",
        "heading--text-size": 26,
        "text--text-size": 18,
        "link": "shopify:\/\/collections\/all",
        "button_label": "Shop now",
        "button-text": "#ffffff",
        "button-background": "#242424",
        "hover-button-text": "#ffffff",
        "hover-button-background": "#9f9f9f"
      }
    },
    "app-product-reviews": {
      "type": "apps",
      "settings": {
